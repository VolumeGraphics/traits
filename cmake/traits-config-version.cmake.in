set(PACKAGE_VERSION @CMAKE_PROJECT_VERSION@)

if(NOT "${PACKAGE_FIND_VERSION_MAJOR}" EQUAL "@CMAKE_PROJECT_VERSION_MAJOR@")
    set(PACKAGE_VERSION_UNSUITABLE TRUE)
elseif("${PACKAGE_FIND_VERSION_MINOR}" GREATER "@CMAKE_PROJECT_VERSION_MINOR@")
    set(PACKAGE_VERSION_UNSUITABLE TRUE)
elseif ("${PACKAGE_FIND_VERSION_MINOR}" EQUAL "@CMAKE_PROJECT_VERSION_MINOR@")
    set(PACKAGE_VERSION_EXACT TRUE)
elseif("${PACKAGE_FIND_VERSION_MAJOR}" EQUAL 0)
    set(PACKAGE_VERSION_UNSUITABLE TRUE)
else()
    set(PACKAGE_VERSION_COMPATIBLE TRUE)
endif()
